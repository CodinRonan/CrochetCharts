<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
   <mime-type type="application/x-stitchworks-pattern">
     <comment>Stitchworks Pattern file</comment>
     <glob pattern="*.pattern"/>
     <magic priority="50">
         <match value="\x95\x9750" type="string" offset="0"/>
     </magic>
     <icon name="stitchworks-pattern"/>
   </mime-type>
</mime-info>

