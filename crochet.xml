<?xml version="1.0"?>
<stitch_set>
    <name>Default Crochet Stitches</name>
    <author>Brian C. Milco</author>
    <email>brian@stitchworkssoftware.com</email>
    <org>Stitch Works Software</org>
    <url>http://www.StitchWorksSoftware.com/</url>

    <stitch>
        <name>x</name>
        <icon>:/stitches/no_stitch.svg</icon>
        <description>this is a place holder stitch</description>
        <category>Basic</category>
        <ws>x</ws>
    </stitch>

    <stitch>
        <name>ss</name>
        <icon>:/stitches/slip.svg</icon>
        <description>slip stitch</description>
        <category>Basic</category>
        <ws>ss</ws>
    </stitch>

    <stitch>
        <name>ch</name>
        <icon>:/stitches/chain.svg</icon>
        <description>chain</description>
        <category>Basic</category>
        <ws>ch</ws>
    </stitch>

    <stitch>
        <name>sc</name>
        <icon>:/stitches/single_crochet.svg</icon>
        <description>single crochet</description>
        <category>Basic</category>
        <ws>sc</ws>
    </stitch>

    <stitch>
        <name>hdc</name>
        <icon>:/stitches/half_double_crochet.svg</icon>
        <description>half double crochet</description>
        <category>Basic</category>
        <ws>hdc</ws>
    </stitch>

    <stitch>
        <name>dc</name>
        <icon>:/stitches/double_crochet.svg</icon>
        <description>double crochet</description>
        <category>Basic</category>
        <ws>dc</ws>
    </stitch>

    <stitch>
        <name>tc</name>
        <icon>:/stitches/treble_crochet.svg</icon>
        <description>treble crochet</description>
        <category>Basic</category>
        <ws>tc</ws>
    </stitch>

    <stitch>
        <name>dtc</name>
        <icon>:/stitches/double_treble_crochet.svg</icon>
        <description>double treble crochet</description>
        <category>Basic</category>
        <ws>dtc</ws>
    </stitch>

    <stitch>
        <name>ttc</name>
        <icon>:/stitches/triple_treble_crochet.svg</icon>
        <description>triple treble crochet</description>
        <category>Basic</category>
        <ws>ttc</ws>
    </stitch>

    <stitch>
        <name>lsdc</name>
        <icon>:/stitches/double_crochet.svg</icon> <!--left_slant_-->
        <description>left slant decrease</description>
        <category>Advanced</category>
        <ws>lsdc</ws>
    </stitch>

    <stitch>
        <name>rsdc</name>
        <icon>:/stitches/double_crochet.svg</icon> <!--right_slant_-->
        <description>right slant double crochet</description>
        <category>Advanced</category>
        <ws>rsdc</ws>
    </stitch>

    <stitch>
        <name>fpdc</name>
        <icon>:/stitches/front_post_dc.svg</icon>
        <description>front post decrease</description>
        <category>Advanced</category>
        <ws>fpdc</ws>
    </stitch>

    <stitch>
        <name>bpdc</name>
        <icon>:/stitches/back_post_dc.svg</icon>
        <description>back post decrease</description>
        <category>Advanced</category>
        <ws>bpdc</ws>
    </stitch>

    <stitch>
        <name>ch3picot</name>
        <icon>:/stitches/ch3_picot.svg</icon>
        <description>chain 3 picot</description>
        <category>Advanced</category>
        <ws>ch3picot</ws>
    </stitch>

    <stitch>
       <name>sc2tog</name>
       <icon>:/stitches/sc2tog.svg</icon>
       <description>single crochet 2 together</description>
       <category>Advanced</category>
       <ws>sc2tog</ws>
    </stitch>

    <stitch>
        <name>dc5shell</name>
        <icon>:/stitches/dc5shell.svg</icon>
        <description>5 double crochet shell</description>
        <category>Advanced</category>
        <ws>dc5shell</ws>
    </stitch>

    <stitch>
       <name>sc3tog</name>
       <icon>:/stitches/sc3tog.svg</icon>
       <description>single crochet 3 together</description>
       <category>Advanced</category>
       <ws>sc3tog</ws>
   </stitch>

   <stitch>
       <name>dc2tog</name>
       <icon>:/stitches/dc2tog.svg</icon>
       <description>double crochet 2 together</description>
       <category>Advanced</category>
       <ws>dc2tog</ws>
   </stitch>

   <stitch>
       <name>dc3tog</name>
       <icon>:/stitches/dc3tog.svg</icon>
       <description>double crochet 3 together</description>
       <category>Advanced</category>
       <ws>dc3tog</ws>
   </stitch>

   <stitch>
       <name>dc3cluster</name>
       <icon>:/stitches/dc3cluster.svg</icon>
       <description>double crochet 3 cluster</description>
       <category>Advanced</category>
       <ws>dc3cluster</ws>
   </stitch>

   <stitch>
       <name>hdc3cluster</name>
       <icon>:/stitches/hdc3cluster.svg</icon>
       <description>bobble</description>
       <category>Advanced</category>
       <ws>hdc3cluster</ws>
   </stitch>

   <stitch>
       <name>dc5popcorn</name>
       <icon>:/stitches/dc5popcorn.svg</icon>
       <description>double crochet 5 popcorn</description>
       <category>Advanced</category>
       <ws>dc5popcorn</ws>
   </stitch>

</stitch_set>
